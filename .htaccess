# RewriteEngine On
# RewriteRule ^(.*)$ public/$1 [L]

 <IfModule mod_rewrite.c>
  RewriteEngine On
  # Force SSL
  RewriteCond %{HTTPS} !=on
  RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
  # Remove public folder form URL
  RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

